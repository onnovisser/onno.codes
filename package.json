    "animejs": "^3.0.1",
    "comlinkjs": "^3.2.0",
    "dat.gui": "^0.7.5",
    "emotion-theming": "^10.0.6",
    "gatsby": "2.1.13",
    "gatsby-image": "^2.0.37",
    "gatsby-mdx": "^0.6.3",
    "gatsby-plugin-catch-links": "^2.0.9",
    "gatsby-plugin-emotion": "^4.0.1",
    "gatsby-plugin-glslify": "1.0.0",
    "gatsby-plugin-lodash": "^3.0.3",
    "gatsby-plugin-manifest": "^2.0.13",
    "gatsby-plugin-netlify": "^2.0.6",
    "gatsby-plugin-offline": "^2.0.21",
    "gatsby-plugin-react-helmet": "^3.0.5",
    "gatsby-plugin-sharp": "^2.0.17",
    "gatsby-plugin-sitemap": "^2.0.4",
    "gatsby-remark-autolink-headers": "^2.0.12",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-source-filesystem": "^2.0.16",
    "gatsby-transformer-sharp": "^2.1.17",
    "glsl-blend-overlay": "^1.0.5",
    "glsl-chromatic-aberration": "^1.0.1",
    "glsl-noise": "^0.0.0",
    "glsl-random": "^0.0.5",
    "glsl-rotate": "^1.0.0",
    "glslify": "^7.0.0",
    "glslify-hex": "^2.1.1",
    "glslify-import": "^3.1.0",
    "lodash": "^4.17.11",
    "mdx-utils": "^0.1.1",
    "mitt": "^1.1.3",
    "prism-react-renderer": "^0.1.5",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-live": "^1.12.0",
    "rxjs": "^6.5.1",
